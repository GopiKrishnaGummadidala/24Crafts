            .l-image-container,.bus_l-image-container {
                 position:relative;
                  margin:20px 10px; 
                 width:164px;
                  float:left;
                  transition:0.5s linear all;
            }
            .l-image-container img,.bus_l-image-container img{ width:100%; transition:0.5s linear all;}
            .l-image-container span,.bus_l-image-container span{
	display:none;
	transition:0.5s linear all;
}
            .l-image-container:hover span,.bus_l-image-container:hover span {
  position: absolute;
  display:block;
  top: 0;
  transition:0.5s linear all;
  z-index: 3;
  cursor: pointer;
  padding: 10px;
}
            .l-image-container:hover span.edit,.bus_l-image-container:hover span.edit { 
                left: 0; 
                background: lightgreen; 
                transition:0.5s linear all;}
            .l-image-container:hover span.delete,.bus_l-image-container:hover span.delete { 
                right: 0;
                 background:red;
                 transition:0.5s linear all; }
            .add { height: 200px; overflow:hidden; background:hsl(0,0%,90%);  

border-radius:7px; padding:7px; }
            .drop_target,.bus_drop_target {
  position: relative;
  cursor: pointer;
  
}
            .drop_target:before,.bus_drop_target:before {
    content: '\f0ee';
    font-family: FontAwesome;
    position: absolute;
    display: block;
    width: 100%;
    line-height: 204px;
    text-align: center;
    font-size: 42px;
    color: #333;
  }
            .drop_target input[type="file"],.bus_drop_target input[type="file"] {
  visibility: hidden;
}
            .image_preview,.bus_image_preview {
    cursor:pointer;
    width: 100%;	
    height: 100%;
    background: no-repeat center;
    background-size: contain;
    position: relative;
    z-index: 2;
  }
            .drop_target:hover, .bus_drop_target:hover {
                opacity: 0.3;
                background-image: url('../../img/default-upload.png');
                background-repeat: no-repeat;
            }

        </style>

